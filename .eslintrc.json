{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "quotes": [2, "double", { "avoidEscape": true }],
        "object-curly-spacing": [2, "always"],
        "new-cap": 0
    }
}
